<%= form_for [@profile, @education] do |f| %>
  <div class="form-group">
    <%= f.label :school %>
    <%= f.text_field :school %>
  </div>
  <div class="form-group">
    <%= f.label :degree %>
    <%= f.text_field :degree %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.textarea :description %>
  </div>
  <div class="form-group">
    <%= f.label :school_url %>
    <%= f.text_field :school_url %>
  </div>
  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
