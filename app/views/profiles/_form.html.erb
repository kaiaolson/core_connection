<%= simple_form_for @profile do |f| %>
  <%= f.input :tagline %>
  <%= f.input :description %>
  <%= f.input :availability %>
  <%= f.input :linkedin_url %>
  <%= f.input :twitter_url %>
  <%= f.input :github_url %>
  <%= f.input :photo, as: :file, multiple: true, hint: 'JPG, JPEG, GIF, PNG' %>
  <%= f.input :resume, as: :file, hint: 'Must be a PDF.' %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
